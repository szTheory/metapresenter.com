%nav.navbar.navbar-expand-lg
  = link_to '/', class: 'navbar-brand' do
    = image_tag 'images/logo.jpeg'
  %button.navbar-toggler{"aria-controls" => "navbarNav", "aria-expanded" => "false", "aria-label" => "Toggle navigation", "data-target" => "#navbarNav", "data-toggle" => "collapse", :type => "button"}
    = icon('bars')
  #navbarNav.collapse.navbar-collapse
    %ul.navbar-nav
      - dato.nav.nav_items.each.with_index do |nav_item, i|
        %li.nav-item
          = link_to nav_item.path, class: 'nav-link' do
            #{nav_item.label}
        - unless i == dato.nav.nav_items.size-1
          %li.nav-item
            %a.nav-link{href: ''}= "//"